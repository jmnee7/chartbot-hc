{
  "version": 2,
  "trailingSlash": false,
  "cleanUrls": true,
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/dashboard", "destination": "/index.html" },
    { "source": "/target", "destination": "/target_index.html" }
  ],
  "redirects": [
    { "source": "/docs/(.*)", "destination": "/$1", "permanent": false }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=300" }
      ]
    }
  ]
}
