{
  "version": 2,
  "trailingSlash": false,
  "builds": [
    { "src": "docs/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/docs/index.html" },
    { "src": "/dashboard", "dest": "/docs/index.html" },
    { "src": "/target", "dest": "/docs/target_index.html" },
    { "src": "/(.*)", "dest": "/docs/$1" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=300" }
      ]
    }
  ]
}
